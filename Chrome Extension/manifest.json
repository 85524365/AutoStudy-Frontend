{
  "manifest_version": 2,
  "name": "公务🐵学习助手",
  "version": "0.0.1",
  "description": "自动学习, 一键🍴🐔",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "icons": {
    "16": "/img/icon16.png",
    "48": "/img/icon48.png",
    "128": "/img/icon128.png"
  },
  "browser_action": {
    "default_icon": "/img/icon128.png",
    "default_title": "为浙江省公务🐵倾情打造❤",
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "permissions": [
    "<all_urls>",
    "tabs",
    "notifications",
    "webRequest",
    "webRequestBlocking",
    "storage"
  ],
  "homepage_url": "https://www.baidu.com"
}