{
  "manifest_version": 2,
  "name": "一键🍴🐔",
  "version": "0.1.1",
  "description": "自动学习程序",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "icons":
  {
      "16": "/img/icon16.png",
      "48": "/img/icon48.png",
      "128":"/img/icon128.png"
  },
  "browser_action": {
    "default_icon": "/img/icon128.png",
    "default_title": "为浙江省公务🐵倾情打造❤",
    "default_popup": "popup.html"
  },
  "permissions": [
    "<all_urls>",
    "contextMenus",
    "tabs",
    "notifications",
    "webRequest",
    "webRequestBlocking",
    "storage"
  ],
  "web_accessible_resources": [],
  "homepage_url": "https://www.baidu.com"
}